{
  "name": "word-finder",
  "version": "1.0.3",
  "description": "find words in English dictionary with autosuggestion!",
  "main": "index.js",
  "devDependencies": {
    "codeclimate-test-reporter": "0.0.4",
    "istanbul": "^0.3.14",
    "jshint": "^2.7.0",
    "pre-commit": "^1.0.7"
  },
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "node test/test.js",
    "nocov": "node test/*.js",
    "coverage": "./node_modules/.bin/istanbul cover test/*.js && ./node_modules/.bin/istanbul check-coverage --statements 100 --functions 100 --lines 100 --branches 100",
    "codeclimate": "CODECLIMATE_REPO_TOKEN=c72e0ce0a9d9768f5ca36e9db0851798f46e726762b68f8103f3027ca361b359 ./node_modules/codeclimate-test-reporter/bin/codeclimate.js < ./coverage/lcov.info",
    "jshint": "jshint -c .jshintrc --exclude-path .gitignore .",
    "start": "node server.js",
    "nodemon": "./node_modules/.bin/nodemon ./server.js"
  },
  "pre-commit": [
    "jshint",
    "coverage"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/jrans/nodediggity.git"
  },
  "keywords": [
    "autocomplete",
    "english",
    "dictionary",
    "suggestions"
  ],
  "author": "jrans",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jrans/nodediggity/issues"
  },
    
  "homepage": "https://github.com/jrans/nodediggity",
  "devDependencies": {
    "codeclimate-test-reporter": "0.0.4",
    "istanbul": "^0.3.14",
    "pre-commit": "1.0.7",
    "qunitjs": "^1.18.0"
  },
  "pre-commit": [
    "jshint",
    "coverage",
    "codeclimate"
  ],
  "engines": {
    "node": ">= 0.10"
  }
}
